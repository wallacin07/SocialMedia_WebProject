            <form action="/postPost/<%= currentUser.idUser %>"  method="post" enctype="multipart/form-data">
                <div>
                    <img class="imgPublicar" id="imgPublicar" src="/img/img_kangoroo/photo.png" alt="post_image.png">
                    
                    <input type="file" name="new_post_image" id="inputPublicar" accept="image/*" onchange="publicar()">
                </div>
        
                <div>
                    <label for="new_post_description">Description:</label>
                    <textarea name="new_post_description" id="new_post_description" class="inputDescricao"></textarea>

    <title>Login</title>
</head>
<body class="">
    <main>
        <img src="/img/logo.png" alt="Logo" id="logo">

        <section class="rect">

            <h1 class="title"><b>Kangoroo</b></h1>

            <form action="" class="form">
                <div class="group">
                    <label for="name">Nome:</label>
                    <input type="text" class="input" id="name" name="name" placeholder="Fulano da Silva">
                </div>
    
                <div class="group">
                    <label for="password">Senha:</label>
                    <input type="password" class="input" id="password" name="password" placeholder="•••••••••••••••">
                </div>
                
                <button type="submit" class="btnEnter"><b>Entrar</b></button>

            </form>

            <div class="botoesinhos">
                <a href="/" id="forgotPass">Esqueceu a senha?</a>
                <a href="/toogleRegister" id="redirectRegister">Não tem uma conta?</a>
            </div>


        </section>
    </main>

    <script src="/js/script.js"></script>
